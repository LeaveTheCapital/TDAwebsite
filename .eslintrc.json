{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["eslint:recommended", "prettier"],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
    },
    "plugins": [
        "html"
    ],
    "settings": {
        "html/indent": "0",
        "html/report-bad-indent": "error"
    }
}
